@using MultivendorWebViewer.ViewModels;
@using MultivendorWebViewer.Common;
@using MultivendorWebViewer.Helpers;

@{
    var dataUrl = ViewBag.DataListUrl as string;
    ViewBag.Title = "Multivendor Web";
    Layout = "~/Views/Shared/_AdministrationLayout.cshtml";
}

<div class="all-user-container">
    @Html.DataView(new DataViewOptions { Url =string.IsNullOrEmpty(dataUrl)? @Url.Action("GetOrderList", "Admin") : dataUrl }, htmlAttributes: new { @class = "user-list-data-view" })
</div>
